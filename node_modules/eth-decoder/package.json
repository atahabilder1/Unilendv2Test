{
  "name": "eth-decoder",
  "version": "0.0.1-beta.2",
  "description": "Simple library to decode ethereum logs and transaction",
  "main": "lib/eth-decoder.js",
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run pack",
    "lint": "eslint src/**/*.js",
    "test": "mocha --require @babel/register --colors ./test/**/*.spec.js",
    "test:watch": "mocha --require @babel/register --colors -w ./test/**/*.spec.js",
    "prepublish": "npm run lint && npm run clean && npm run build",
    "pack": "webpack && webpack --env build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maticnetwork/eth-decoder.git"
  },
  "keywords": [
    "ethereum",
    "web3",
    "ethers",
    "truffle"
  ],
  "author": "Jaynti Kanani <jd@matic.network>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maticnetwork/eth-decoder/issues"
  },
  "homepage": "https://github.com/maticnetwork/eth-decoder#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "ethers": "^4.0.10",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
